		h4 Sidebar

		.row.mb-5
			.col-md-4.col-sm-6.mb-3
				a.card(href="sidebar.html" target="_blank")
					.card-body
						.card-text Sidebar
						small.card-text A simple sidebar menu example
			
			.col-md-4.col-sm-6.mb-3
				a.card(href="sidebar-right.html" target="_blank")
