block content
	.navbar.navbar-expand-lg.bsnav.bsnav-brand-center

		a.navbar-brand(href="#") Navbar

		button.navbar-toggler.toggler-spring
			span.navbar-toggler-icon


		.collapse.navbar-collapse.justify-content-md-between
			ul.navbar-nav
				li.nav-item.active
					a.nav-link(href="#") Home
				li.nav-item
					a.nav-link(href="#") About Us
				li.nav-item
					a.nav-link(href="#") News


			ul.navbar-nav
				li.nav-item
					a.nav-link(href="#") Item
				li.nav-item
					a.nav-link(href="#") Gallery
				li.nav-item
					a.nav-link(href="#") Contact


	.jumbotron.jumbotron-fluid
		.container
			h1 Navbar center brand


	style.
		.jumbotron{
			background: url('https://picsum.photos/1400/700?image=823') no-repeat center top / cover;
			color: #fff;
			height: 600px;
			display: flex;
			align-items: center;
		}